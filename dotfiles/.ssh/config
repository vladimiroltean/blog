# Titel Petrescu stuff

# Raspberry Pi
Host alarmpi
	User alarm
	HostName skbuf.go.ro
	Port 3928
	Compression yes
	ForwardX11 yes
	ForwardX11Trusted yes
# Used to forward SSH (22) port from this machine to Raspberry Pi port 60000
Host reverse-alarmpi
	RemoteForward 60000 localhost:22
	HostName skbuf.go.ro
	Port 3928
	User alarm
	ForwardX11 yes
	Compression yes
	TCPKeepAlive yes
	ServerAliveInterval 60
# Used to connect from alarmpi to whomever tunneled their SSH port
# Can be used only from alarmpi physical network
Host tunnel-alarmpi
	HostName alarmpi
	Port 60000
	User b56933
	ForwardX11 yes
	Compression yes
	TCPKeepAlive yes
	ServerAliveInterval 60

